# Android fstab file.
#   <src>		          <mnt_point> <type>	<mnt_flags>					                          	<fs_mgr_flags>
# load these in early-init instead

/dev/block/mmcblk0p25   /system   ext4	ro,noatime,data=ordered,noauto_da_alloc              wait
/dev/block/mmcblk0p27   /data     ext4	noatime,nosuid,nodev,data=ordered,noauto_da_alloc    wait,encryptable=footer
/dev/block/mmcblk0p26   /cache    ext4	noatime,nosuid,nodev,barrier=1,noauto_da_alloc,errors=panic,journal_async_commit       wait,check
/dev/block/mmcblk0p28   /devlog   ext4	noatime,nosuid,nodev,data=ordered,noauto_da_alloc    wait
/dev/block/mmcblk0p20   /boot     emmc  defaults                                             defaults
/dev/block/mmcblk0p21   /recovery emmc  defaults                                             defaults
/dev/block/mmcblk0p22   /misc     emmc  defaults                                             defaults

# sdcard
/devices/platform/msm_sdcc.3/mmc_host/mmc1  auto  auto  defaults  voldmanaged=sdcard0:auto,noemulatedsd
/devices/platform/msm_hsusb.0/usb1          auto  auto  defaults  voldmanaged=usbdisk:auto
